algoritmo "treno 01"
var
money: Real
inicio
      Escreva("Quanto de dinheiro eu tenho? ")
      leia(money)
      Se money >= 10000 entao
         Escreval("Partio disney")
      senao
           se (money >= 5000) e (money < 10000) entao
              EscrevaL("Visita a família")
           senao
                Escreval("#chateado")
           fimse
      fimse
fimalgoritmo
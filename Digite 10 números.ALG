algoritmo "Digite 10 números"
var
conte,soma, mais: inteiro
inicio
soma <- 0
Enquanto conte < 5 faca
         conte <- conte + 1
         Escreva ("Digite o", conte, "° valor: ")
         leia(mais)
         soma <- soma + mais
FimEnquanto
EscrevaL("")
EscrevaL("A soma de todos os valores foi", soma)
fimalgoritmo
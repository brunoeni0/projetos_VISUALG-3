algoritmo "faz soma procedimento"
var
   x,y: inteiro
   Procedimento soma(A, B: inteiro)
   inicio
   EscrevaL ("Recerbi o valor ", A)
   EscrevaL ("Recerbi o valor ", B)
   EscrevaL ("A soma entre os dois é ", A + B)
   FimProcedimento
inicio
      x <- 5
      y <- 3
      soma(x, y)
fimalgoritmo